{
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "streamlit_app.py"
    }
  ],
  "env": {
    "ASSEMBLYAI_API_KEY": "@assemblyai_api_key",
    "OPENROUTER_API_KEY": "@openrouter_api_key"
  }
}